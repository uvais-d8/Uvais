        }

        @keyframes php-email-form-loading {
            0% {
                transform: rotate(0deg);
            }

            100% {
                transform: rotate(360deg);
            }
        }

        /*--------------------------------------------------------------
# Global Header
--------------------------------------------------------------*/
        .header {
            --background-color: #000000;
            --default-color: #ffffff;
            --heading-color: #ffffff;
            color: var(--default-color);
            background-color: var(--background-color);
